{
  "name": "Mohsen-template",
  "version": "1.0.0",
  "description": "This is a great template for express.js, which makes it easy for developers.",
  "main": "main.js",
  "scripts": {
    "start": "NODE_ENV=production node main.js",
    "migrate": "cross-env NODE_ENV=production node app/DataBase/migrate.js",
    "migrate:force": "cross-env NODE_ENV=production FORCE=true node app/DataBase/migrate.js",
    "dev": "cross-env NODE_ENV=development nodemon ./bin/www",
    "dev:migrate": "cross-env NODE_ENV=development node app/DataBase/migrate.js",
    "dev:migrate:force": "cross-env NODE_ENV=development FORCE=true node app/DataBase/migrate.js",
    "test": "cross-env NODE_ENV=test node main.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "mysql",
    "restapi"
  ],
  "author": "Mohsen-ziadi",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^14.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "multer": "^1.4.4",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.20",
    "sequelize": "^6.15.0",
    "short-hash": "^1.0.0",
    "socket.io": "^4.0.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  }
}
